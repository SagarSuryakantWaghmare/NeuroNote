# Production Environment Variables Template
# Copy this to .env and fill in your actual values

# Database Configuration (MongoDB Atlas)
MONGODB_URL=mongodb+srv://username:password@cluster.mongodb.net/database_name

# JWT Secret (Use a strong, unique secret)
JWT_PASSWORD=your_super_secret_jwt_key_here_make_it_long_and_complex

# Server Configuration
PORT=3000
NODE_ENV=production

# CORS Configuration (Your frontend domain)
FRONTEND_URL=https://your-neuronote-app.vercel.app
